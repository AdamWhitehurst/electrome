{
  "name": "electrome",
  "version": "0.0.1",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=icon.ico --prune=true --out=release-builds",
    "package-win": "electron-packager . electrome --overwrite --asar=true --platform=win32 --arch=ia32 --icon=icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electrome\"",
    "package-linux": "electron-packager . electrome --overwrite --asar=true --platform=linux --arch=x64 --icon=icon.ico  --prune=true --out=release-builds"
  },
  "keywords": [],
  "author": "Adam Whitehurst",
  "license": "ISC",
  "devDependencies": {
    "electron": "^4.0.4",
    "electron-packager": "^13.0.1"
  },
  "dependencies": {
    "pixi.js": "^4.8.5"
  }
}
